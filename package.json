{
  "name": "pgintro",
  "version": "0.1.1",
  "description": "Introspect a Postgres database and generate TypeScript types.",
  "main": "dist/introspect.js",
  "scripts": {
    "build": "rm -rf dist/* && tsc && cp src/introspect.sql dist/",
    "test": "jest --coverage",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "postgres",
    "database",
    "codegen"
  ],
  "author": "Jason Jones <jay@subdes.co>",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "pg": "^7.4.0",
    "pluralize": "^7.0.0",
    "ts-simple-ast": "^4.2.1",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "@types/find-up": "^2.1.1",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^22.0.0",
    "@types/lodash": "^4.14.91",
    "@types/node": "^8.5.2",
    "@types/pg": "^7.4.1",
    "@types/pluralize": "0.0.28",
    "@types/underscore.string": "0.0.32",
    "dotenv": "^4.0.0",
    "find-up": "^2.1.0",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true,
    "coverageDirectory": ".coverage"
  },
  "files": [
    "dist"
  ]
}
